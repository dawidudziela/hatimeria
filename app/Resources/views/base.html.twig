<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Hatimeria CMF{% endblock %}</title>
        <link rel="stylesheet" href="{{ asset('bundles/hatimeriaadmin/resources/css/my-ext-theme.css') }}" type="text/css" media="all" />
        <link rel="stylesheet" href="{{ asset('bundles/hatimeriaadmin/resources/css/ext-custom.css') }}" type="text/css" media="all" />
        {% render "HatimeriaExtJSBundle:Javascript:headers" %}
        {% block head %}{% endblock %}
    </head>
    <body>
        <div id="body"></div>
        {% block body %}{% endblock %}
        {% block fos_user_content %}{% endblock %}
        {% block javascripts %}{% endblock %}
        {% render "HatimeriaExtJSBundle:Javascript:variables" %}
    </body>
</html>
